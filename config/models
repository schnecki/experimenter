Exp         -- Adding "json" causes ToJSON and FromJSON instances to be derived.
    name Text
    startTime UTCTime
    endTime UTCTime Maybe
    initialState ByteString
    initialInputState ByteString
    deriving Eq Ord

Param
    exp ExpId
    name Text
    minBound ByteString
    maxBound ByteString
    deriving Eq Ord

ExpSetup
    exp ExpId
    repetitions Int
    preparationSteps Int Maybe
    evaluationWarmUpSteps Int Maybe
    evaluationSteps Int
    evaluationReplications Int
    maximumParallelEvaluations Int
    UniqueExpSetup exp

ExpResult
    exp ExpId
    repetition Int

ExpResultData
    expResult                       ExpResultId
    startTime                       UTCTime
    endTime                         UTCTime Maybe
    startRandGen                    Text
    endRandGen                      Text Maybe
    startState                      ByteString
    endState                        ByteString Maybe
    startInputState                 ByteString
    endInputState                   ByteString Maybe
    UniquePrepResultDataExpResult   expResult

ParamSetting
    expResult ExpResultId
    name Text
    value ByteString

PrepInput
    expResult ExpResultId
    period Int

PrepInputValue
    prepInput PrepInputId
    value ByteString

PrepMeasure
    expResult ExpResultId
    period Int

PrepResultStep
    measure PrepMeasureId
    name Text
    xValue  Double Maybe
    yValue  Double

RepResult
    expResult ExpResultId
    repNr Int

WarmUpResultData
    repResult                       RepResultId
    startTime                       UTCTime
    endTime                         UTCTime Maybe
    startRandGen                    Text
    endRandGen                      Text Maybe
    startState                      ByteString
    endState                        ByteString Maybe
    startInputState                 ByteString
    endInputState                   ByteString Maybe
    UniqueWarmUpResultDataRepResult repResult


RepResultData
    repResult                       RepResultId
    startTime                       UTCTime
    endTime                         UTCTime Maybe
    startRandGen                    Text
    endRandGen                      Text Maybe
    startState                      ByteString
    endState                        ByteString Maybe
    startInputState                 ByteString
    endInputState                   ByteString Maybe
    UniqueRepResultDataRepResult    repResult


WarmUpInput
    repResult RepResultId
    period Int

WarmUpInputValue
    warmUpInput WarmUpInputId
    value ByteString

WarmUpMeasure
    repResult RepResultId
    period Int

WarmUpResultStep
    measure WarmUpMeasureId
    name Text
    xValue  Double Maybe
    yValue  Double

RepInput
    repResult RepResultId
    period Int

RepInputValue
    repInput RepInputId
    value ByteString

RepMeasure
    repResult RepResultId
    period Int

RepResultStep
    measure RepMeasureId
    name Text
